  //注册微服务consul配置
  "servicecenter": {
    "consul": {
      "Enable": false,
      "ConsulEndpoint": "http://127.0.0.1:8500",
      "ServiceHost": "127.0.0.1",
      "ServicePort": "5260",
      "ServiceName": "ServiceA",
      "GroupName": "ServiceA",
      "Tags": "1"
    },
    "nacos": {
      "Enable": false,
      "Namespace": "public",
      "ServiceName": "ServiceA",
      "EndPoint": "",
      "ServerAddresses": [ "http://localhost:8848" ],
      "DefaultTimeOut": 10000,
      "ListenInterval": 1000,
      "GroupName": "DEFAULT_GROUP",
      "ClusterName": "DEFAULT",
      "Ip": "",
