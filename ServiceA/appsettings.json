{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  //本地日志
  "LocalLog": {
    "Enable": true,
    //日志保留天数
    "SaveDays": 30,
    //日志文件绝对路径，不设置默认放在程序主目录
    "LogFilePath": ""
  },

  //注册微服务consul配置
  "consul": {
    "Enable": true,
    "ConsulEndpoint": "http://127.0.0.1:8500",
    "ServiceHost": "127.0.0.1",
    "ServicePort": "5228",
    "ServiceName": "ServiceA",
    "GroupName": "ServiceA",
    "Tags": "1"
  }
}
